<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto"

    >

    <TextView
        android:id="@+id/myPage_login_nickName_tv"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:textSize="16sp"
        android:text="닉네임"
        android:textColor="@color/black"
        android:textStyle="bold"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        />
    <TextView
        android:id="@+id/myPage_login_follower_tv"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:textSize="14sp"
        android:text="12"
        android:textColor="@color/black"
        android:textStyle="bold"
        app:layout_constraintTop_toBottomOf="@+id/myPage_login_nickName_tv"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginTop="13dp"
        />
    <TextView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:textSize="14sp"
        android:text="팔로워"
        android:textColor="@color/black"
        app:layout_constraintTop_toBottomOf="@+id/myPage_login_follower_tv"
        app:layout_constraintStart_toStartOf="parent"
        />
    <TextView
        android:id="@+id/myPage_login_following_tv"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:textSize="14sp"
        android:text="12"
        android:textColor="@color/black"
        android:textStyle="bold"
        app:layout_constraintTop_toBottomOf="@+id/myPage_login_nickName_tv"
        app:layout_constraintStart_toEndOf="@+id/myPage_login_follower_tv"
        android:layout_marginStart="60dp"
        android:layout_marginTop="13dp"
        />
    <TextView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:textSize="14sp"
        android:text="팔로잉"
        android:textColor="@color/black"
        app:layout_constraintTop_toBottomOf="@+id/myPage_login_following_tv"
        app:layout_constraintStart_toEndOf="@+id/myPage_login_follower_tv"
        android:layout_marginStart="60dp"
        />









</androidx.constraintlayout.widget.ConstraintLayout>